INSERT INTO `element` VALUES (1, 0, 0, 'beach_panorama2.jpg', 'top', '<b>Fictional Farm</b> is the place to take the whole family for a relaxing break away from the stresses and strains of work, housekeeping and school. <br><br>At <b>Fictional Farm</b>, we pride ourselves in having everything you&#039;ll need to have a great holiday.<br><br>In a beautiful setting, with all our facilities on-site and the beach little more than a stone&#039;s throw away, <b>Fictional Farm Holiday Cottages</b> offer luxurious and relaxing surroundings in five-star comfort for the discerning holidaymaker.<br><br>', '', 'Y', now(), 'root', null);

