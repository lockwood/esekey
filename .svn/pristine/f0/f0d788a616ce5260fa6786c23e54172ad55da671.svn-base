A:link    {color: green;
           text-decoration: none;}

A:visited {color: red;
           text-decoration: none;}

A:hover  {color: black;
           text-decoration: underline;
           cursor: hand;}

td          {font-family: Verdana, "Times New Roman", Times;
             font-size: 10pt;}

td.lefthead {background-color: green;
             color: #ECEAFF;
             width: 15%;
             height: 36px; 
             text-align: right;
             font-weight: bold;}

td.midhead {background-color: lightgreen;
             color: darkgreen;
             height: 36px;
             text-align: left;
             font-weight: bold;}

td.righthead {background-color: lightgreen;
             color: darkgreen;
             width: 15%;
             height: 36px;
             text-align: center;
             vertical-align: middle;}

td.indexcol  {background-color: #BBFF88;
             width: 20%;
             font-size: 7.5pt;
             font-weight: normal;
             text-align: center;
             vertical-align: top;}

td.leftcol  {background-color: #BBFF88;
             font-size: 7.5pt;
             font-weight: normal;
             text-align: center;
             vertical-align: top;}

td.midcol   {text-align: left;
             vertical-align: top;}

td.rightcol {background-color: #BBFF88;
             text-align: center; 
             font-size: 7.5pt;
             vertical-align: top;}

td.leftfoot {background-color: green;
             color: #ECEAFF;
             height: 20px;}

td.rightfoot {background-color: green;
             color: #ECEAFF;
             height: 20px;
             text-align: left;
             vertical-align: center;}
