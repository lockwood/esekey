A:link    {color: green;
           text-decoration: none;}

A:visited {color: green;
           text-decoration: none;}

A:hover  {color: black;
           text-decoration: underline;
           cursor: hand;}

td          {font-family: Georgia, Verdana, Times;
             font-size: 10pt;}

td.lefthead {background-color: #DDFFFF;
             color: black;
             width: 14%;
             height: 24px; 
             vertical-align: bottom;
             text-align: right;
             font-weight: bold;}

td.midline {background-color: brown;
             color: #000070;
             height: 2px;
             vertical-align: bottom;
             text-align: left;
             font-weight: bold;}

td.midhead {background-color: #333366;
             color: #ECEAFF;
             vertical-align: bottom;
             text-align: left;
             font-weight: bold;}

td.righthead {color: #000070;
             width: 14%;
             height: 3px;
             vertical-align: bottom;
             text-align: left;}

td.leftcol  {background-image: url(maidenh1.jpg);
             font-weight: bold;
             text-align: right;
             vertical-align: top;}

td.midcol   {background-image: url(maidenh1.jpg);
             text-align: left;
             vertical-align: top;}

td.rightcol {text-align: center; 
             font-size: 7.5pt;
             vertical-align: top;}

td.contact { text-align: center; 
             font-size: 8pt;
             vertical-align: top;}

td.leftfoot {color: #333366;
             height: 20px;}

td.rightfoot {color: #333366;
             font-size: 8pt;
             height: 20px;
             text-align: left;
             vertical-align: center;}
td.currstep  {background-color: steelblue;
             font-size: 8pt;
             font-weight: bold; 
   		 font-style: italic;
             text-align: left;
             vertical-align: center;}

td.otherstep  {background-color: lightblue;
             font-size: 8pt;
             font-weight: bold; 
		 font-style: italic; 
             text-align: left;
             vertical-align: center;}

td.compstep  {background-color: lightgreen;
             font-size: 8pt;
             font-weight: bold; 
		 font-style: italic; 
             text-align: left;
             vertical-align: center;}
