body      {color: #000000;
           margin: 0px;
           padding: 0px;
           font-family: Verdana, "Times New Roman", Times;
           font-size: 9pt;}

#mainbody  {position: absolute;
           left: 1%;
           top: 1%;
           height: 98%;
           width: 98%;}

.currentsection  {background-color: steelblue;
             font-size: 10pt;
             font-weight: bold; 
   		 font-style: italic;
             height: 22px;
             text-align: center;
             vertical-align: center;}

.othersection  {background-color: lightblue;
             font-size: 10pt;
             font-weight: bold; 
		 font-style: italic; 
             height: 22px;
             text-align: center;
             vertical-align: center;}

